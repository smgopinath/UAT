CREATE TABLE t_pc_nps_holdings (
  nps_holdings_id bigint NOT NULL AUTO_INCREMENT,
  tracking_id bigint DEFAULT NULL COMMENT 'Customer Id',
  scheme_preference_type varchar(70) DEFAULT NULL COMMENT 'Scheme preference',
  amount_in_transition double DEFAULT NULL COMMENT 'Amount Transition',
  nav varchar(45) DEFAULT NULL COMMENT 'Net Asset Value',
  scheme_name varchar(120) DEFAULT NULL COMMENT 'Type of Scheme',
  scheme_id varchar(70) DEFAULT NULL COMMENT 'Scheme Id',
  allocation_perc varchar(10) DEFAULT NULL COMMENT 'Allocation Percaentage',
  total_units double DEFAULT NULL COMMENT 'Total Unit',
  total_value_of_scheme double DEFAULT NULL COMMENT 'Total Value of Scheme',
  created_by varchar(100) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  modified_by varchar(100) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  created_date datetime(6) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  modified_date datetime(6) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  consent_id varchar(100) DEFAULT NULL COMMENT 'Reference Id from Primid',
  current_value double DEFAULT NULL COMMENT 'Current Value',
  active bit(1) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  tier_type varchar(10) DEFAULT NULL,
  PRIMARY KEY (nps_holdings_id),
  KEY fk_tracking_id_idx (tracking_id) /*!80000 INVISIBLE */,
  KEY fk_tracking_id_idx2 (tracking_id,consent_id),
  CONSTRAINT fk_tracking_id1 FOREIGN KEY (tracking_id) REFERENCES t_customer (id)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE t_pc_nps_summary (
  nps_summary_id bigint NOT NULL AUTO_INCREMENT,
  tracking_id bigint DEFAULT NULL COMMENT 'Customer Id',
  name varchar(40) DEFAULT NULL COMMENT 'Customer Name',
  pranId varchar(40) DEFAULT NULL COMMENT 'Permanent Retirement Account Number',
  opening_date date DEFAULT NULL COMMENT 'Account Openeed Date',
  current_value double DEFAULT NULL COMMENT 'Current Value',
  debt_asset_value varchar(40) DEFAULT NULL COMMENT 'Debt Asset Value    ',
  equity_asset_value varchar(40) DEFAULT NULL COMMENT 'Equioty Asset Value ',
  other_asset_value varchar(40) DEFAULT NULL COMMENT 'Other Asset Value   ',
  created_by varchar(100) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  modified_by varchar(100) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  created_date datetime(6) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  modified_date datetime(6) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  consent_id varchar(100) DEFAULT NULL COMMENT 'Reference Id from Primid',
  active bit(1) DEFAULT NULL COMMENT 'Audit Columns used for traceability',
  refresh_date datetime(6) DEFAULT NULL,
  next_refresh_date datetime(6) DEFAULT NULL,
  linkedAccRef varchar(50) DEFAULT NULL,
  acct_holder_name varchar(50) DEFAULT NULL COMMENT 'Account Holder Name',
  PRIMARY KEY (nps_summary_id),
  KEY fk_pc_nps_summary (tracking_id),
  CONSTRAINT fk_pc_nps_summary FOREIGN KEY (tracking_id) REFERENCES t_customer (id)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
